<%= form_for @movie do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
  
    <%= f.label :genre %>
    <%= f.text_field :genre %>
  
    <%= f.label :picture %>
    <%= f.text_field :picture %>

    <%= f.label :synopsis %>
    <%= f.text_field :synopsis %>

    <%= f.label :year %>
    <%= f.text_field :year %>
  
    <%= f.submit "Create" %>
  <% end %>